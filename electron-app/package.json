{
  "name": "GRAPEVNE",
  "version": "0.3.1",
  "description": "GRAPEVNE electron app",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "lint": "eslint src/**/*.{ts,tsx}",
    "build": "tsc",
    "test": "jest ./src",
    "postbuild-tests": "jest --runInBand ./postbuild_tests",
    "postbuild-tests-no-containers": "jest --runInBand ./postbuild_tests --no-containers",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' 'postbuild_tests/*.{ts,tsx,js}'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "author": "kraemer-lab",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-dmg": "^7.3.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/publisher-github": "^6.2.1",
    "@types/decompress": "^4.2.5",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.3.1",
    "@types/selenium-webdriver": "^4.1.16",
    "@types/shelljs": "^0.8.13",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "babel-jest": "^29.5.0",
    "decompress": "^4.2.1",
    "electron": "27",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.33.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "selenium-webdriver": "^4.12.0",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "webpack": "^5.88.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "electron-chromedriver": "27",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "node-pty": "^1.0.0",
    "python-shell": "^5.0.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.0.2"
}
